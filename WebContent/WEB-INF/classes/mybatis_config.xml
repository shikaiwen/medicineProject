<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<environments default="development">
		<environment id="development">
			<transactionManager type="JDBC" />
			<dataSource type="POOLED">
				<property name="driver" value="oracle.jdbc.driver.OracleDriver"/>
				<property name="url" value="jdbc:oracle:thin:1521:orcl" />
				<property name="username" value="drp" />
				<property name="password" value="drp" />
			</dataSource>
		</environment>
	</environments>
	
	
	<mappers>
		<mapper resource="com/drp/mapper/Dict_mapper.xml"/>
		<mapper resource="com/drp/mapper/User_mapper.xml"/>
		<mapper resource="com/drp/mapper/Agent_mapper.xml"/>
		<mapper resource="com/drp/mapper/Medicine_mapper.xml"/>
		<mapper resource="com/drp/mapper/Order_mapper.xml"/>
		<mapper resource="com/drp/mapper/Store_mapper.xml"/>
		<mapper resource="com/drp/mapper/FlowCard_mapper.xml"/>
		<mapper resource="com/drp/mapper/Report_mapper.xml"/>
	</mappers>
	
</configuration>